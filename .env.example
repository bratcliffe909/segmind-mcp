# Segmind API Configuration
# Get your API key from https://segmind.com
SEGMIND_API_KEY=your_segmind_api_key_here

# Optional: Server Configuration
NODE_ENV=production
LOG_LEVEL=info

# Optional: Default Models
DEFAULT_TEXT2IMG_MODEL=sdxl
DEFAULT_IMG2IMG_MODEL=sdxl-img2img
DEFAULT_VIDEO_MODEL=veo-3

# Optional: Performance Settings
CACHE_ENABLED=true
CACHE_TTL=3600
MAX_BATCH_SIZE=5
REQUEST_TIMEOUT=120000